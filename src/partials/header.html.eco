<div id="top">
	<header role="banner">
		<h1><a href="/">EduanTech</a></h1>
	</header>
	<nav role="navigation">
		<% for page in @menuItems.findAllLive({menuShow:true,ignored:false},[{menuOrder:1}]).toJSON(): %>
			<a href="<%= page.url %>"<%= if page.title is @document.title then ' id=menu-current' %>>
				<%= page.title %>
			</a>
		<% end %>
	</nav>
</div>
